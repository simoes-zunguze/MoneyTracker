{
   "compilerOptions": {
      "lib": [
         "ES2018",
      ],
      
      "target": "ES2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop":true,
      "strict": true,
      "strictPropertyInitialization": false,
      "allowJs": true,
   }
}